baselines
baseline: spec

	<baseline>
	spec for: #common do: [ 
		spec
			package: 'Fmpython';
			package: 'PythonToPharoTranscompiler'.
		spec
			baseline: 'PythonParser'
			with: [ spec repository: 'github://SmaCCRefactoring/SmaCC' ].
		spec baseline: 'FAST' with: [ 
			spec
				loads: #( 'default' );
				repository: 'github://moosetechnology/FAST:v1.x.x/src' ] ]